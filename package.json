{
  "name": "formscaffold.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3799",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:terminal": "docker exec -it formscaffold-postgres psql -U formscaffold_user -d formscaffold_db",
    "db:setup": "tsx src/scripts/setup-database.ts",
    "db:test": "tsx src/scripts/test-database.ts",
    "db:init": "tsx src/scripts/generate-db-init.ts",
    "db:start": "docker-compose -f .docker/docker-compose.yml up -d",
    "db:stop": "docker-compose -f .docker/docker-compose.yml down",
    "db:restart": "npm run db:stop && npm run db:start",
    "db:logs": "docker-compose -f .docker/docker-compose.yml logs -f",
    "db:reset": "docker exec -i formscaffold-postgres psql -U formscaffold_user -d formscaffold_db < init-scripts/01-init-tables.sql",
    "generate:all": "tsx src/scripts/generate-all.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/themes": "^3.2.1",
    "@tanstack/react-query": "^5.84.1",
    "@trpc/client": "^11.4.3",
    "@trpc/next": "^11.4.3",
    "@trpc/react-query": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "clsx": "^2.1.1",
    "next": "15.4.5",
    "postgres": "^3.4.7",
    "prettier": "^3.6.2",
    "radix-ui": "^1.4.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "sonner": "^2.0.7",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
